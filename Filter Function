import random
import numpy as np
def fun(integer):
    number = np.arange(1,100)
    if (integer in number):
        return True
    else:
        return False
        
        
Sequence = [1,200,34,56,222,53,67,98,13243]
filtered = filter(fun, Sequence)

print('the filtered numbers are :')

for a in (filtered):
    print(a)
    
    
#Ouput will include = 'the filtered numbers are' - 1,34,56,67,98
